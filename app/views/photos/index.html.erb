<%= content_for(:title, "Photo Sharing!") %>
<%#NOTE: MAIN PAGE IS USERS INDEX. THIS PROVIDED FOR ILLUSTRATIVE PURPOSES ONLY %>

<h1> Photo Sharing! </h1>

<% @users.each do |user| %>
  <%= user.name %>
  <ul class="pics">
  <% user.photos.each do |photo| %>
          <li>
          <%= link_to image_tag(photo.image.url(:thumb)), user_photo_path(user, photo) %>
          </li>
  <% end %>
  </ul>
<% end %>


<%= link_to "Sign Up", new_user_path, :class => "btn btn-default" %>

<%= link_to "Log In", new_session_path :class => "btn btn-default" %>